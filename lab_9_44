#include <stdio.h>

int main() {
    float a, b, c, d;

    // Ввод длин отрезков
    printf("Введите длины отрезков a, b, c, d: ");
    scanf("%f %f %f %f", &a, &b, &c, &d);

    // Проверка условий для построения параллелограмма
    if ((a == c && b == d) || (a == d && b == c)) {
        printf("Можно построить параллелограмм.\n");
    } else {
        printf("Нельзя построить параллелограмм.\n");
    }

    return 0;
}
