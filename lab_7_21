def main():
    # Ввод размера массива
    N = int(input("Введите размер массива (N): "))
    
    # Ввод элементов массива
    A = []
    print("Введите элементы массива:")
    for _ in range(N):
        A.append(int(input()))

    # Ввод значений K и L
    K = int(input("Введите K (1 ≤ K ≤ N): "))
    L = int(input("Введите L (K ≤ L ≤ N): "))

    # Проверка корректности значений K и L
    if 1 <= K <= N and K <= L <= N:
        # Суммируем элементы от K до L (преобразуем K и L в индексы массива)
        total_sum = sum(A[K-1:L])  # Индексы в Python начинаются с 0
        print(f"Сумма элементов с номерами от {K} до {L}: {total_sum}")
    else:
        print("Ошибка: значения K и L должны быть в пределах от 1 до N.")

if __name__ == "__main__":
    main()
